{
  "name": "mmry-raindrop",
  "private": true,
  "version": "0.0.1",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "janniks",
  "license": "MIT",
  "mmry": {
    "name": "Raindrop.io Import",
    "description": "Import bookmarks from Raindrop.io",
    "store": {
      "icon": "data:image/svg+xml;base64,",
      "bg": "#4086D9",
      "tagline": "Import your Raindrop.io bookmarks"
    },
    "permissions": [
      "create-item"
    ],
    "defaultCollection": {
      "name": "raindrop",
      "color": "#4086D9"
    },
    "defaultCron": "0 0 * * *",
    "type": "interval",
    "inputs": [
      {
        "id": "RAINDROP_API_TOKEN",
        "name": "Raindrop API Token",
        "type": "string",
        "description": "Your Raindrop.io API test token. Generate one at https://app.raindrop.io/settings/integrations"
      }
    ]
  }
}
