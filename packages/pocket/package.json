{
  "name": "mmry-pocket",
  "version": "0.0.1",
  "description": "A mmry plugin for Pocket",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "janniks",
  "license": "MIT",
  "mmry": {
    "name": "Pocket Bookmarks Sync",
    "description": "Pocket plugin for syncing bookmarks to mmry",
    "store": {
      "icon": "data:image/svg+xml;base64,",
      "bg": "#D54D57",
      "tagline": "Sync your Pocket bookmarks"
    },
    "permissions": [
      "create-item"
    ],
    "type": "interval",
    "env": [
      {
        "id": "POCKET_API_KEY",
        "name": "Pocket API Key",
        "type": "string",
        "description": "The consumer key for the Pocket API. You can get it from https://getpocket.com/developer/apps/new"
      }
    ]
  }
}
