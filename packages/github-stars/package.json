{
  "name": "mmry-github-stars",
  "private": true,
  "version": "0.0.1",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "janniks",
  "license": "MIT",
  "mmry": {
    "name": "GitHub Stars",
    "description": "Sync your GitHub starred repositories",
    "store": {
      "icon": "data:image/svg+xml;base64,",
      "bg": "#222222",
      "tagline": "Sync your GitHub starred repositories"
    },
    "permissions": [
      "create-item"
    ],
    "allow-net": [
      "api.github.com"
    ],
    "defaultCollection": {
      "name": "github:repository",
      "color": "#f59e0b"
    },
    "defaultCron": "0 0 * * *",
    "type": "interval",
    "inputs": [
      {
        "id": "GITHUB_TOKEN",
        "name": "GitHub Personal Access Token",
        "type": "string",
        "description": "Your GitHub Personal Access Token with 'Starring' read permissions. Generate one at https://github.com/settings/tokens"
      }
    ]
  }
}
