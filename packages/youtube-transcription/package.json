{
  "name": "mmry-youtube-transcription",
  "private": true,
  "version": "0.0.1",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "janniks",
  "license": "MIT",
  "mmry": {
    "name": "YouTube Transcription",
    "description": "Transcribe YouTube videos",
    "store": {
      "icon": "data:image/svg+xml;base64,",
      "bg": "#ff0000",
      "tagline": "Transcribe YouTube videos"
    },
    "permissions": [
      "create-item"
    ],
    "allow-net": [
      "www.youtube-transcript.io",
      "identitytoolkit.googleapis.com",
      "youtube.com"
    ],
    "defaultCollection": {
      "id": "youtube",
      "name": "YouTube",
      "color": "#ff0000"
    },
    "type": "trigger",
    "inputs": [
      {
        "id": "youtube-video",
        "name": "YouTube Video URL",
        "type": "text",
        "description": "The YouTube video URL to transcribe"
      }
    ]
  }
}
